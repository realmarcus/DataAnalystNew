mutate(df, Colour = ifelse(grepl("purpal", Colour), "purple",Colour))
mutate(df, Colour = ifelse(grepl("purpal", Colour), "Purple",
ifelse(grepl("purple",Colour)),"Purple",
ifelse(grepl("red",Colour),"Red",
ifelse(grepl("blue",Colour),"Blue","Green"))))
mutate(df, Colour = ifelse(grepl("purpal", Colour), "Purple","tata"))
df
